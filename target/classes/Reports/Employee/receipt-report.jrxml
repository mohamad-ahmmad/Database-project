	<queryString>
		<![CDATA[
SELECT d.DRESSID, d.DRESSNAME, DNAME,CITY,
 RECEPIT_NUM,(  SELECT sum( d.PRICE*( 1 -SALEPERCENTAGE/100)) AS Total_price  FROM SELL_RECORD sd 
 JOIN DRESS d ON sd.DRESSID = d.DRESSID JOIN 
 DEPARTMENT_DRESS dd on d.DRESSID = dd.DRESSID and sd.DEPID=dd.DID
  where  RECEPIT_NUM = (SELECT MAX(RECEPIT_NUM) FROM SELL_RECORD)) as TotalPrice,
 sd.AMOUNT , d.PRICE, SALEPERCENTAGE , d.PRICE*( 1 -SALEPERCENTAGE/100) AS Price_Offer  FROM SELL_RECORD sd 
 JOIN DRESS d ON sd.DRESSID = d.DRESSID JOIN 
 DEPARTMENT_DRESS dd on d.DRESSID = dd.DRESSID and dd.DID = sd.DEPID join DEPARTMENT d on d.DID=dd.DID
  where  RECEPIT_NUM = (SELECT MAX(RECEPIT_NUM) FROM SELL_RECORD)

  
  ]]>
	</queryString>
